{
  "name": "enumerate-cli",
  "version": "0.0.1",
  "description": "Command line interface for enumerate",
  "contributors": [
    "Jeremy Nauta <jeremypnauta@gmail.com>"
  ],
  "repository": "enumeratejs/enumerate-cli",
  "license": "MIT",
  "engines": {
    "node": ">= 4.0"
  },
  "main": "index.js",
  "bin": {
    "enumerate": "bin/enumerate.js"
  },
  "files": [
    "index.js",
    "lib",
    "bin"
  ],
  "scripts": {
    "lint": "eslint index.js bin/ src/",
    "transpile": "rm -rf lib && babel src --out-dir lib",
    "prepare": "npm run transpile",
    "test": "npm run transpile && mocha --async-only",
    "cover": "istanbul cover _mocha --report lcovonly"
  },
  "dependencies": {
    "ansi-colors": "~1.0.1",
    "archy": "~1.0.0",
    "array-sort": "~1.0.0",
    "color-support": "~1.1.3",
    "concat-stream": "~1.6.0",
    "copy-props": "~2.0.1",
    "enumerate": "~0.0.0",
    "fancy-log": "~1.1.0",
    "interpret": "~1.0.0",
    "isobject": "~3.0.1",
    "js-yaml": "~3.10.0",
    "liftoff": "~2.3.0",
    "matchdep": "~2.0.0",
    "mocha": "~3.5.3",
    "mute-stdout": "~1.0.0",
    "pretty-hrtime": "~1.0.0",
    "replace-homedir": "~1.0.0",
    "v8flags": "~3.0.1",
    "yargs": "~10.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "expect": "22.1.0",
    "istanbul": "0.4.4",
    "mocha": "3.5.3"
  }
}
